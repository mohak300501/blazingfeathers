{
  "name": "blazing-feathers",
  "version": "1.0.0",
  "description": "A beautiful bird photography webapp with Firebase authentication and Google Drive integration",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "npm run install:functions && vite build",
    "preview": "vite preview",
    "lint": "eslint . --ext js,jsx,ts,tsx",
    "lint:fix": "eslint . --ext js,jsx,ts,tsx --fix",
    "build:netlify": "vite build",
    "install:functions": "cd netlify/functions && npm install && cd ../.."
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.8.1",
    "firebase": "^10.7.1",
    "googleapis": "^128.0.0",
    "lucide-react": "^0.294.0",
    "date-fns": "^2.30.0",
    "react-hot-toast": "^2.4.1"
  },
  "netlifyFunctions": {
    "firebase-admin": "^11.11.0",
    "googleapis": "^128.0.0"
  },
  "devDependencies": {
    "@types/react": "^18.2.43",
    "@types/react-dom": "^18.2.17",
    "@typescript-eslint/eslint-plugin": "^6.14.0",
    "@typescript-eslint/parser": "^6.14.0",
    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "^10.4.16",
    "eslint": "^8.55.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "postcss": "^8.4.32",
    "tailwindcss": "^3.3.6",
    "typescript": "^5.2.2",
    "vite": "^5.0.8"
  },
  "keywords": [
    "birds",
    "photography",
    "firebase",
    "react",
    "netlify"
  ],
  "author": "Blazing Feathers Team",
  "license": "MIT",
  "engines": {
    "node": ">=18.0.0"
  }
} 